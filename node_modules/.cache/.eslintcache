  {\r\n          point: updatedPoint,\r\n        }\r\n      );\r\n\r\n      // Navigasi kembali ke halaman utama setelah berhasil\r\n      navigate(\"/\");\r\n    } catch (error) {\r\n      console.error(error);\r\n    }\r\n  };\r\n\r\n  const [selectedDate, setSelectedDate] = useState(null);\r\n\r\n  const handleDateChange = (date) => {\r\n    setSelectedDate(date);\r\n  };\r\n\r\n  return (\r\n    <div\r\n      className=\"box\"\r\n      style={{\r\n        width: \"50%\",\r\n        margin: \"0 auto\",\r\n        marginTop: \"50px\",\r\n        paddingBottom: \"70px\",\r\n      }}\r\n    >\r\n      <div className=\"columns mt-5 is-centered\">\r\n        <div className=\"column is-half\">\r\n          <form onSubmit={savePelanggaran}>\r\n            <div className=\"field\">\r\n              <label className=\"label\">Input Tanggal</label>\r\n              <div className=\"control\">\r\n                <DatePicker\r\n                  selected={selectedDate}\r\n                  className=\"input\"\r\n                  onChange={handleDateChange}\r\n                  dateFormat=\"dd/MM/yyyy\" // Format tampilan tanggal dengan jam, menit, dan detik\r\n                  isClearable // Membuat opsi untuk menghapus tanggal\r\n                />\r\n              </div>\r\n            </div>\r\n            <div className=\"field\">\r\n              <label className=\"label\">Keterangan</label>\r\n              <div className=\"control\">\r\n                <input\r\n                  type=\"text\"\r\n                  className=\"input\"\r\n                  value={keterangan}\r\n                  onChange={(e) => setKeterangan(e.target.value)}\r\n                  placeholder=\"Keterangan\"\r\n                />\r\n              </div>\r\n            </div>\r\n            <div className=\"field\">\r\n              <label className=\"label\">Point</label>\r\n              <div className=\"control\">\r\n                <input\r\n                  type=\"text\"\r\n                  className=\"input\"\r\n                  value={Kurangpoint}\r\n                  onChange={(e) => setKurangpoint(e.target.value)}\r\n                  placeholder=\"Point\"\r\n                />\r\n              </div>\r\n            </div>\r\n            <div className=\"field\">\r\n              <button className=\"button is-success\" type=\"submit\">\r\n                Tambah\r\n              </button>\r\n              <Link to={\"/\"} className=\"button is-normal is-danger ml-2\">\r\n                Menu Utama\r\n              </Link>\r\n            </div>\r\n          </form>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default AddPelanggaran;\r\n",{"ruleId":"156","severity":1,"message":"157","line":16,"column":5,"nodeType":"158","endLine":16,"endColumn":7,"suggestions":"159"},{"ruleId":"160","severity":1,"message":"161","line":6,"column":10,"nodeType":"162","messageId":"163","endLine":6,"endColumn":14},{"ruleId":"156","severity":1,"message":"157","line":17,"column":6,"nodeType":"158","endLine":17,"endColumn":8,"suggestions":"164"},{"ruleId":"160","severity":1,"message":"161","line":6,"column":10,"nodeType":"162","messageId":"163","endLine":6,"endColumn":14},{"ruleId":"160","severity":1,"message":"165","line":8,"column":10,"nodeType":"162","messageId":"163","endLine":8,"endColumn":21},{"ruleId":"160","severity":1,"message":"166","line":8,"column":23,"nodeType":"162","messageId":"163","endLine":8,"endColumn":37},{"ruleId":"156","severity":1,"message":"157","line":17,"column":6,"nodeType":"158","endLine":17,"endColumn":8,"suggestions":"167"},{"ruleId":"156","severity":1,"message":"157","line":16,"column":5,"nodeType":"158","endLine":16,"endColumn":7,"suggestions":"168"},{"ruleId":"156","severity":1,"message":"157","line":16,"column":5,"nodeType":"158","endLine":16,"endColumn":7,"suggestions":"169"},{"ruleId":"160","severity":1,"message":"161","line":6,"column":10,"nodeType":"162","messageId":"163","endLine":6,"endColumn"